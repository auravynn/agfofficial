  <script>
    // Theme toggle (dark/light switch)
    function toggleTheme() {
      const html = document.documentElement;
      const isDark = html.getAttribute('data-theme') === 'dark';
      html.setAttribute('data-theme', isDark ? 'light' : 'dark');
      document.querySelector('.theme-toggle').textContent = isDark ? 'Dark Mode' : 'Light Mode';
    }

    // Scroll progress rainbow
    window.addEventListener('scroll', () => {
      const scrolled = (window.scrollY / (document.body.scrollHeight - window.innerHeight)) * 100;
      document.getElementById('scroll-rainbow').style.width = scrolled + '%';
    });

    // Back comet visibility
    window.addEventListener('scroll', () => {
      document.querySelector('.back-comet').classList.toggle('show', window.scrollY > 800);
    });

    // Neon particles canvas
    const canvas = document.getElementById('neon-void');
    const ctx = canvas.getContext('2d');
    canvas.width = window.innerWidth;
    canvas.height = window.innerHeight;

    const particlesArray = [];
    const particleNum = 150;
    const neonColors = ['#00d4ff', '#c300ff', '#ff2d55', '#39ff14', '#00faff'];

    class NeonParticle {
      constructor() {
        this.x = Math.random() * canvas.width;
        this.y = Math.random() * canvas.height;
        this.size = Math.random() * 4 + 1;
        this.speedX = Math.random() * 0.8 - 0.4;
        this.speedY = Math.random() * 0.8 - 0.4;
        this.color = neonColors[Math.floor(Math.random() * neonColors.length)];
        this.opacity = Math.random() * 0.6 + 0.3;
      }
      update() {
        this.x += this.speedX;
        this.y += this.speedY;
        if (this.x < 0 || this.x > canvas.width) this.speedX *= -1;
        if (this.y < 0 || this.y > canvas.height) this.speedY *= -1;
      }
      draw() {
        ctx.globalAlpha = this.opacity;
        ctx.fillStyle = this.color;
        ctx.shadowBlur = 20;
        ctx.shadowColor = this.color;
        ctx.beginPath();
        ctx.arc(this.x, this.y, this.size, 0, Math.PI * 2);
        ctx.fill();
      }
    }

    for (let i = 0; i < particleNum; i++) particlesArray.push(new NeonParticle());

    function animateVoid() {
      ctx.fillStyle = 'rgba(0,3,10,0.08)';
      ctx.fillRect(0, 0, canvas.width, canvas.height);
      particlesArray.forEach(p => { p.update(); p.draw(); });
      requestAnimationFrame(animateVoid);
    }
    animateVoid();

    window.addEventListener('resize', () => {
      canvas.width = window.innerWidth;
      canvas.height = window.innerHeight;
    });

    // Stat counter animation
    document.querySelectorAll('.stat-num').forEach(el => {
      const target = parseInt(el.getAttribute('data-target')) || 100;
      let count = 0;
      const increment = target / 80;
      const timer = setInterval(() => {
        count += increment;
        if (count >= target) {
          count = target;
          clearInterval(timer);
        }
        el.textContent = Math.floor(count) + (target === Infinity ? '' : '+');
      }, 40);
    });
  </script>

</body>
</html>
